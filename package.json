{
  "name": "free-dash-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "A monorepo for the financial dashboard project.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:client": "npm start --workspace=@free-dash/client",
    "dev:api": "npm run dev --workspace=@free-dash/api",
    "build:client": "npm run build --workspace=@free-dash/client",
    "build:api": "npm run build --workspace=@free-dash/api",
    "build:shared": "npm run build --workspace=@free-dash/shared-types",
    "migrate:dev": "npm run migrate:dev --workspace=@free-dash/api"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "prisma": "^6.12.0"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
